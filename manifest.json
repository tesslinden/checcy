{
  "name": "checcy",
  "version": "1.0",
  "description": "Flips between prod and local URLs when the extension button is clicked.",
  "manifest_version": 3,
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "checcy",
    "default_icon": "checcy_icon.png"
  }
}
